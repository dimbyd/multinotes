    \ifcat\char\space%
        \let\next=\multinotes@remove%
    \else%
        \ifcat\char x%
            \let\next=\multinotes@remove%
        \else%
            \ifcat\char 1%
                \let\next=\multinotes@remove%
            \else%
                \let\next=\relax%
            \fi%
        \fi%
    \fi%
    \next%
