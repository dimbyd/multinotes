            \topsep=2pt\partopsep=0pt\choiceshook
        }
    }{\if@correctchoice\endgroup\fi\endlist}
    \newenvironment{checkboxes}{
        \list{\checkbox@char}{
            \usecounter{choice}
            \settowidth{\leftmargin}{W.\hskip\labelsep}
